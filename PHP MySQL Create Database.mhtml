.w3-note, .w3-warning { border-left: none; }

.w3-panel { margin: 24px -32px; padding: 16px 32px; }

h1 { font-size: 42px; }

h2 { font-size: 32px; }

.w3-btn:hover, .w3-btn:active, .w3-example a:focus, .nextprev a:focus { box-shadow: none; background-color: rgb(70, 160, 73) !important; }

.w3-btn:hover.w3-blue, .w3-btn:active.w3-blue, .w3-button:hover.w3-blue, .w3-button:active.w3-blue { background-color: rgb(13, 139, 242) !important; color: rgb(255, 255, 255) !important; }

