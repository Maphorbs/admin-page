#main { padding: 16px 32px 32px; border-right: 1px solid rgb(241, 241, 241); }

#right { padding: 16px 8px; }

.sidesection .w3-left-align { text-align: center !important; }

#footer { padding: 32px; border-top: 1px solid rgb(241, 241, 241); }

